<!DOCTYPE html>
<html>
<head>
  <title>ZenAppointments</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
	<!-- Flash Messages -->
	<div class="row">
		<div class="col-xs-12">
			<% flash.each do |key, value| %>
				<div class="alert alert-<%= key %>">
					<%= value %>
				</div>
			<% end %>
		</div>
	</div>
	<!-- Page Content -->
	<%= yield %>
</div>


</body>
</html>